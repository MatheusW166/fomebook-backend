{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "quotes": [
            "error",
            "double"
        ],
        "import/extensions": [
            "off"
        ],
        "no-console": [
            "off"
        ],
        "import/prefer-default-export": [
            "off"
        ],
        "no-underscore-dangle": [
            "off"
        ],
        "no-param-reassign": [
            "off"
        ]
    }
}